{
  "presets": [
    "env",
    {
      "modules": false
    }
  ],
  "plugins": [
    "external-helpers",
    [
      "transform-runtime",
      {
        "polyfill": true,
        "regenerator": true
      }
    ],
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ],
    "add-module-exports"
  ]
}
