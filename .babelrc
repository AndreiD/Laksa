{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "targets": {
          "browsers": [">0.25%"]
        },
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/transform-runtime",
    ],
    "@babel/proposal-object-rest-spread",
    "@babel/proposal-export-default-from",
    "@babel/proposal-export-namespace-from",
    "@babel/proposal-class-properties",
    "add-module-exports"
  ],
  "overrides": [
    {
      "exclude": [
        "//node_modules$/"
      ]
    }
  ],
}
